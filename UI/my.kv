
<StartWindow>:

    name: "start"

    FloatLayout:
        Label:
            pos_hint:{"x":0.1,"y":0.8}
            size_hint: 0.8, 0.2
            font_size: 25
            text: 'Gas Blending'


        Button:
            pos_hint:{"x":0,"y":0.05}
            size_hint: 0.4, 0.2
            font_size: (root.width**2 + root.height**2) / 17**4
            text: "X"
            on_release:
                root.manager.transition.direction = "right"
                root.submit1()
        
        Button:
            pos_hint:{"x":0,"y":0.3}
