<HomeScreen>:
    FloatLayout:
        canvas:
            Color:
                rgb: 1,1,1,1
            Rectangle:
                size: self.size
                pos: self.pos




        GridLayout:
            rows: 1
            pos_hint: {'top': 1, 'left': 1}
            size_hint: 1, .2
            Image:
                source: 'pic/gasblendpic.png'



        GridLayout:
            rows: 1
            pos_hint: {'top': 0.8, 'left': 1}
            size_hint: 1, .2
            ImageButton:
                source: 'pic/gasblend1.png'
                on_release:
                    print('GasBlend')



        GridLayout:
            rows: 1
            pos_hint: {'top': 0.6, 'left': 1}
            size_hint: 1, .2
            ImageButton:
                source: 'pic/profile.png'
                on_release:
                    print('Profile')


        GridLayout:
            rows: 1
            pos_hint: {'top': 0.4, 'left': 1}
            size_hint: 1, .2
            ImageButton:
                source: 'pic/settings.png'
                on_release:
                    print('Settings')